<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/navigation_main"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.zatch.navigation.main.MainFragment"
        android:label="MainFragment"
        tools:layout="@layout/fragment_main">
        <action
            android:id="@+id/action_mainFragment_to_moreGatchFragment"
            app:destination="@id/moreGatchFragment" />
    </fragment>
    <fragment
        android:id="@+id/moreGatchFragment"
        android:name="com.example.zatch.navigation.main.MoreGatchFragment"
        android:label="MoreGatchFragment"
        tools:layout="@layout/fragment_more_gatch">
    </fragment>
</navigation>