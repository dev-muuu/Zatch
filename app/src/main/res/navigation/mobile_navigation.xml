<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.zatch.navigation.main.MainFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_main" />

    <fragment
        android:id="@+id/navigation_zatch"
        android:name="com.example.zatch.navigation.my_zatch.MyZatchFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_my_zatch" />

    <fragment
        android:id="@+id/navigation_chat"
        android:name="com.example.zatch.navigation.chat.ChatFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_chat" />
    <fragment
        android:id="@+id/navigation_user"
        android:name="com.example.zatch.navigation.user_page.UserPageFragment"
        android:label="fragment_user_page"
        tools:layout="@layout/fragment_user_page" />
</navigation>