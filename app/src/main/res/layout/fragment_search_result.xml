<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/s1"
        android:layout_width="match_parent"
        android:layout_height="116dp">

        <TextView
            android:id="@+id/textView41"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="16dp"
            android:drawableLeft="@drawable/size_filter_alt_20"
            android:drawablePadding="4dp"
            android:fontFamily="@font/pretendard_medium"
            android:text="검색필터"
            android:textSize="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/imageView16" />

        <ImageView
            android:id="@+id/imageView16"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginTop="36dp"
            android:layout_marginBottom="56dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/horizontal_switch_light" />

        <TextView
            android:id="@+id/searchTownFilterButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginBottom="12dp"
            android:drawableRight="@drawable/size_arrow_down_20"
            android:drawablePadding="4dp"
            android:text="홍제동"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/c1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginRight="12dp"
            android:layout_marginBottom="2dp"
            app:layout_constraintBottom_toBottomOf="@id/imageView16"
            app:layout_constraintRight_toLeftOf="@id/imageView16"
            app:layout_constraintTop_toTopOf="parent">

            <ImageButton
                android:id="@+id/categoryCheckButton1"
                android:layout_width="28dp"
                android:layout_height="16dp"
                android:background="@drawable/selector_category_is_choosed"
                app:layout_constraintEnd_toEndOf="@+id/myRegisterField"
                app:layout_constraintStart_toStartOf="@+id/myRegisterField" />

            <TextView
                android:id="@+id/myRegisterField"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_marginTop="14dp"
                android:fontFamily="@font/pretendard_bold"
                android:gravity="center"
                android:text="생수수수"
                android:textSize="16dp"
                app:layout_constraintTop_toBottomOf="@id/categoryCheckButton1" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <EditText
            android:id="@+id/myZatchChangeSearch"
            android:layout_width="0dp"
            android:layout_height="32dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="12dp"
            android:layout_marginBottom="20dp"
            android:background="@drawable/search_change"
            android:ems="10"
            android:gravity="center"
            android:inputType="textPersonName"
            android:paddingLeft="18dp"
            android:paddingTop="6dp"
            android:paddingRight="18dp"
            android:paddingBottom="6dp"
            android:textColor="@color/black_10"
            android:textColorHint="@color/black_10"
            android:textCursorDrawable="@drawable/drawble_cursor_edit_text"
            android:textSize="16dp"
            android:textStyle="bold"
            android:visibility="gone"
            app:layout_constraintBottom_toTopOf="@id/textView41"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@id/imageView16" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="12dp"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="2dp"
            app:layout_constraintBottom_toBottomOf="@id/imageView16"
            app:layout_constraintLeft_toRightOf="@id/imageView16"
            app:layout_constraintTop_toTopOf="parent">

            <ImageButton
                android:id="@+id/categoryCheckButton2"
                android:layout_width="28dp"
                android:layout_height="16dp"
                android:background="@drawable/selector_category_is_choosed"
                app:layout_constraintEnd_toEndOf="@+id/wantField"
                app:layout_constraintStart_toStartOf="@+id/wantField" />

            <TextView
                android:id="@+id/wantField"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_marginTop="14dp"
                android:fontFamily="@font/pretendard_bold"
                android:gravity="center"
                android:text="라며녀녀녀"
                android:textSize="16dp"
                app:layout_constraintTop_toBottomOf="@id/categoryCheckButton2" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <EditText
            android:id="@+id/wantZatchChangeSearch"
            android:layout_width="0dp"
            android:layout_height="32dp"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="20dp"
            android:background="@drawable/search_change"
            android:ems="10"
            android:gravity="center"
            android:inputType="textPersonName"
            android:paddingLeft="18dp"
            android:paddingTop="6dp"
            android:paddingRight="18dp"
            android:paddingBottom="6dp"
            android:textColor="@color/black_10"
            android:textColorHint="@color/black_10"
            android:textCursorDrawable="@drawable/drawble_cursor_edit_text"
            android:textSize="16dp"
            android:textStyle="bold"
            android:visibility="gone"
            app:layout_constraintBottom_toTopOf="@id/textView41"
            app:layout_constraintLeft_toRightOf="@id/imageView16"
            app:layout_constraintRight_toRightOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="wrap_content"
        android:layout_height="8dp"
        app:layout_constraintTop_toBottomOf="@id/s1"
        app:srcCompat="@drawable/drawble_main_page_sector_divider" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/searchZatchRecyclerview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="visible"
        app:layout_constraintTop_toBottomOf="@id/imageView15" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/noSearchResultLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        app:layout_constraintTop_toBottomOf="@id/imageView15">

        <TextView
            android:id="@+id/textView35"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="214dp"
            android:layout_marginBottom="244dp"
            android:fontFamily="@font/pretendard_bold"
            android:gravity="center"
            android:text="이런! \n검색결과가 없어요."
            android:textSize="18dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView36"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="27dp"
            android:layout_marginTop="84dp"
            android:layout_marginRight="30dp"
            android:text="검색 TIP"
            android:textColor="@color/black_85"
            android:textSize="16dp"
            android:textStyle="bold"
            app:layout_constraintRight_toLeftOf="@id/textView35"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/textView35" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="22dp"
            android:background="@drawable/drawble_background_no_result_text"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/textView36">

            <TextView
                android:id="@+id/textView37"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:layout_marginTop="12dp"
                android:layout_marginEnd="31dp"
                android:fontFamily="@font/pretendard_regular"
                android:text="\u2022 교환하고 싶은 재치가 있다면 나의 재치를 ‘?‘ 로 설정하고 검색해서 상대방에게 제안해보세요!"
                android:textColor="@color/black_85"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:lineHeight="19.6dp" />

            <TextView
                android:id="@+id/textView38"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dp"
                android:layout_marginBottom="12dp"
                android:fontFamily="@font/pretendard_regular"
                android:text="\u2022 유연한 탐색을 통해 우리 집 근처의 재치들을 둘러보세요!"
                android:textColor="@color/black_85"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="@id/textView37"
                app:layout_constraintRight_toRightOf="@id/textView37"
                app:layout_constraintTop_toBottomOf="@id/textView37"
                app:lineHeight="19.6dp" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>