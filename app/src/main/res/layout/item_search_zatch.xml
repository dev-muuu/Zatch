<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="138dp">

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="18dp"
            android:background="#B8B5B5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <CheckBox
            android:id="@+id/checkBox2"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/selector_heart_button_yellow"
            android:button="@null"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView28"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="24dp"
            android:text="삼다수 생수 1L"
            android:textStyle="bold"
            app:layout_constraintLeft_toRightOf="@id/imageView11"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginTop="4dp"
            app:layout_constraintLeft_toLeftOf="@id/textView28"
            app:layout_constraintTop_toBottomOf="@id/textView28"
            app:srcCompat="@drawable/exchange_silver" />

        <TextView
            android:id="@+id/textView29"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:text="신라면 3봉"
            android:textStyle="bold"
            app:layout_constraintLeft_toLeftOf="@id/textView28"
            app:layout_constraintTop_toBottomOf="@id/imageView12" />

        <TextView
            android:id="@+id/textView30"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="불광동"
            app:layout_constraintLeft_toLeftOf="@id/textView28"
            app:layout_constraintTop_toBottomOf="@id/textView29" />

        <TextView
            android:id="@+id/textView31"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="2dp"
            android:text="냥냥이"
            app:layout_constraintLeft_toRightOf="@id/textView32"
            app:layout_constraintTop_toTopOf="@id/textView30" />

        <TextView
            android:id="@+id/textView32"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="2dp"
            android:text="·"
            app:layout_constraintLeft_toRightOf="@id/textView30"
            app:layout_constraintTop_toTopOf="@id/textView30" />

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:layout_marginTop="102dp"
            android:layout_marginRight="4dp"
            android:layout_marginBottom="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toLeftOf="@id/textView34"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/heart_silver" />

        <TextView
            android:id="@+id/textView34"
            android:layout_width="wrap_content"
            android:layout_height="13dp"
            android:layout_marginTop="1.5dp"
            android:layout_marginRight="8dp"
            android:text="15"
            android:textColor="@color/black_20"
            android:textSize="10dp"
            app:layout_constraintRight_toLeftOf="@id/imageView13"
            app:layout_constraintTop_toTopOf="@id/imageView14" />

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:layout_marginRight="4dp"
            app:layout_constraintRight_toLeftOf="@id/textView33"
            app:layout_constraintTop_toTopOf="@id/imageView14"
            app:srcCompat="@drawable/chat_alt_2" />

        <TextView
            android:id="@+id/textView33"
            android:layout_width="wrap_content"
            android:layout_height="13dp"
            android:layout_marginLeft="4dp"
            android:layout_marginTop="1.5dp"
            android:layout_marginEnd="20dp"
            android:text="13"
            android:textColor="@color/black_20"
            android:textSize="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintLeft_toRightOf="@id/imageView13"
            app:layout_constraintTop_toTopOf="@id/imageView14" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>