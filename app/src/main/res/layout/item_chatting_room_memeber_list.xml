<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="33.65dp"
        android:layout_height="33.65dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="9.18dp"
        android:layout_marginTop="9.18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/profile_yellow" />

    <ImageView
        android:id="@+id/crownImage"
        android:layout_width="12.94dp"
        android:layout_height="12.94dp"
        android:layout_marginTop="22dp"
        app:layout_constraintEnd_toEndOf="@id/imageView19"
        app:layout_constraintTop_toTopOf="@id/imageView19"
        app:srcCompat="@drawable/crown_button" />

    <TextView
        android:id="@+id/memeberName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginLeft="9.5dp"
        android:text="ìˆ‘"
        app:layout_constraintLeft_toRightOf="@id/imageView19" />

    <TextView
        android:id="@+id/isOwner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/text_background_stroke_yel_10"
        android:fontFamily="@font/pretendard_regular"
        android:text="ME"
        android:textColor="@color/zatch_yellow"
        android:textSize="12dp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/memberDeclarationButton"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/report" />
</androidx.constraintlayout.widget.ConstraintLayout>